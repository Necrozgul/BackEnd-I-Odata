<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="API"
                   namespace="API">

	<class name="Device">
		<id name="Id">
			<generator class="guid" />
		</id>
		<property name="Name" />
		<property name="Date" />
		<property name="Price" />
		<property name="State" />
	</class>

</hibernate-mapping>